<footer>
  <div class="inner">
	<div class="pure-g">
		<div class="pure-u-1 pure-u-md-1-6">
			<nav class="horizontal-list newsletter fullwidth">
				<ul>
					<li>
					</li>
				</ul>
			</nav>
		</div>
      <div class="pure-u-1 pure-u-md-2-3">
      <nav class="horizontal-list tertiary-menu">
		<ul>
            {% for link in linklists[settings.footer_linklist].links %}
              <li><a href="{{ link.url }}" title="{{ link.title }}">{{ link.title }}</a></li>
            {% endfor %}
		</ul>
	</nav>
      </div>
		<div class="pure-u-1 pure-u-md-1-6">
			<nav class="horizontal-list social">
				<ul>
					<li><a href="{{ settings.instagram_link }}" class="" target="_blank"><span class="typcn typcn-social-instagram"></span></a></li>
                    <li><a href="{{ settings.facebook_link }}" class="" target="_blank"><span class="typcn typcn-social-facebook"></span></a></li>
					<li><a href="{{ settings.pinterest_link }}" class="" target="_blank"><span class="typcn typcn-social-pinterest"></span></a></li>
                  <li><a href="{{ settings.twitter_link }}" class="" target="_blank"><span class="typcn typcn-social-twitter"></span></a></li>
				</ul>
              <div class="addthis_custom_sharing"></div>
			</nav>
		</div>
	</div>
	
  </div>
  
</footer>

